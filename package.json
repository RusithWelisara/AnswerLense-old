    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.18.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tesseract.js": "^6.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
